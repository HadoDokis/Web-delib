<?xml version="1.0" encoding="UTF-8"?>
<schema targetNamespace="http://gedooo/CaddieService" xmlns="http://www.w3.org/2001/XMLSchema" xmlns:gedooo="http://gedooo/CaddieService">
	<element name="CaddieService">
		<complexType>
			<sequence>
				<element ref="gedooo:parametrageCaddie" maxOccurs="1"
					minOccurs="0">
				</element>
				<choice maxOccurs="1" minOccurs="1">

					<element name="deposerFichier" maxOccurs="unbounded"
						minOccurs="1">
						<complexType>
							<sequence maxOccurs="1" minOccurs="1">
								<element name="caddieCible"
									type="gedooo:caddieType" maxOccurs="unbounded" minOccurs="1">
								</element>
								<element name="fichierADeposer"
									type="gedooo:fichierDocumentType" maxOccurs="unbounded"
									minOccurs="1">
								</element>
							</sequence>
						</complexType>
					</element>

					<element name="supprimerFichier">
						<complexType>
							<sequence maxOccurs="unbounded"
								minOccurs="0">
								<element name="caddieCible"
									type="gedooo:caddieType">
								</element>
								<element name="nomFichierASupprimer"
									type="string">
								</element>
							</sequence>
						</complexType>
					</element>
					<element name="listerFichier">
						<complexType>
							<sequence maxOccurs="unbounded"
								minOccurs="0">
								<element name="caddieCible"
									type="gedooo:caddieType">
								</element>
							</sequence>
						</complexType>
					</element>
                    <element name="retirerFichier">
						<complexType>
							<sequence maxOccurs="unbounded" minOccurs="0">
								<element name="caddieCible" type="gedooo:caddieType">
								</element>
								<element name="nomFichierARetirer" type="string">
								</element>
							</sequence>
						</complexType>
					</element>
				</choice>
			</sequence>
		</complexType>
	</element>

	<complexType name="fichierDocumentType">
		<sequence maxOccurs="1" minOccurs="1">
			<element name="fichier" type="base64Binary"></element>
			<element name="nomFichier" type="string"></element>
			<element name="contentType" type="string"></element>
		</sequence>
	</complexType>

	<element name="parametrageCaddie">
		<complexType>
			<sequence>
			</sequence>
		</complexType>
	</element>

	<complexType name="caddieType">
		<sequence maxOccurs="1" minOccurs="1">
			<element name="nom" type="string"></element>
			<element name="repertoire" type="string"></element>
		</sequence>
	</complexType>

	<complexType name="droitsCaddieType">
		<sequence>
			<element name="modfication" type="boolean"></element>
			<element name="suppression" type="boolean"></element>
			<element name="creation" type="boolean"></element>
		</sequence>
	</complexType>

	<complexType name="utilisateurType"></complexType>
	

</schema>